<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
	<RunConfiguration>
		<MaxCpuCount>4</MaxCpuCount>
		<TargetFrameworkVersion>.NETCoreApp,Version=v8.0</TargetFrameworkVersion>
		<TestSessionTimeout>60000</TestSessionTimeout>
	</RunConfiguration>

	<Execution>
		<Parallelize>
			<Workers>4</Workers>
			<Scope>Class</Scope>
		</Parallelize>
	</Execution>
</RunSettings>